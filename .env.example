# Environment Configuration for UniPack API
# Copy this file to .env and fill in the values

# Application
APP_NAME="UniPack API"
APP_VERSION="1.0.0"
DEBUG=false

# NocoDB Configuration
NOCODB_URL=https://noco.unipack.asia
NOCODB_API_TOKEN=your_nocodb_api_token_here
NOCODB_BASE_ID=your_base_id_here

# Odoo Configuration
ODOO_URL=https://erp.unipack.asia
ODOO_DB=unipack
ODOO_USER=hello@unipack.asia
ODOO_PASSWORD=your_odoo_password_here
